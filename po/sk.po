#: ../src/meego-panel-people.c:88 ../src/mnb-people-panel.c:476
#: ../data/meego-panel-people.desktop.in.in.h:2
msgid "people"
msgstr "ľudia"

#: ../src/mnb-people-panel.c:57
msgid "Hello! This is the People panel. When you have an IM service configured (like Messenger), this is where you chat to people."
msgstr "Toto je panel ľudí. Keď si nastavíte službu IM budete môcť hovoriť s ľuďmi."

#: ../src/mnb-people-panel.c:191 ../src/mnb-people-panel.c:414
#, c-format
msgid "Open %s"
msgstr "Otvoriť %s"

#: ../src/mnb-people-panel.c:231
msgid "Sorry, we can't find any people. Have you set up a Messenger account?"
msgstr "Prepáčte, nie je možné nájsť žiadnych ľudí. Už ste si nastavili komunikátor?"

#: ../src/mnb-people-panel.c:283
msgid "Sorry, we can't find any people. It looks like they are all offline."
msgstr "Prepáčte, nie je možné nájsť žiadnych ľudí. Vyzerá to tak,že nikto nie je pripojený."

#: ../src/mnb-people-panel.c:328
msgid "To see your IM contacts, you need to go online."
msgstr "Musíte byť pripojený, aby ste uvideli vaše IM kontakty."

#: ../src/mnb-people-panel.c:458
#, c-format
msgid "people - you are chatting with %d people"
msgstr "ľudia - hovoríte s %d ľuďmi"

#: ../src/mnb-people-panel.c:465
msgid "people - you are chatting with someone"
msgstr "ľudia - hovoríte s niekým"

#. Populate header
#: ../src/mnb-people-panel.c:669 ../src/mnb-people-panel.c:923
msgid "People"
msgstr "Ľudia"

#: ../src/mnb-people-panel.c:682
msgid "Search"
msgstr "Hľadať"

#: ../src/mnb-people-panel.c:733 ../src/mnb-people-panel.c:1122
msgid "You are chatting with:"
msgstr "Hovoríte s:"

#: ../src/mnb-people-panel.c:940
msgid "Me and my people"
msgstr "Ja a moji ľudia"

#: ../src/mnb-people-panel.c:978
msgid "Me"
msgstr "Ja"

#. Side table
#: ../src/mnb-people-panel.c:1079
msgid "My conversations"
msgstr "Moje rozhovory"

#: ../data/meego-panel-people.desktop.in.in.h:1
msgid "People panel"
msgstr "Panel ľudí"
